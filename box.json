{
    "compression": "GZ",
    "files": [
        "bootstrap.php",
        "LICENSE"
    ],
    "finder": [
        {
            "in": ["src"],
            "notName": [
                "*Test.php",
                "*TestCase.php"
            ]
        },
        {
            "in": ["vendor"]
        }
    ],
    "git-commit-short": "git_version",
    "intercept": true,
    "output": "stackwatch.phar",
    "stub": true
}
